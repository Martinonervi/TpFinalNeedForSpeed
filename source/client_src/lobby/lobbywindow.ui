         <string notr="true">/* Fondo global fijo */
QMainWindow#LobbyWindow {
    background-image: url(&quot;:/images/menu_bg_clean.png&quot;);
    background-repeat: no-repeat;
    background-position: center;
    background-color: black;
}

/* Páginas transparentes para que se vea el fondo */
QWidget#LobbySelector, QWidget#MainMenu { background: transparent; }

/* ===== Panel del lobby ===== */
QFrame#panel {
    background: rgba(8, 12, 20, 220);     /* negro azulado semi */
    border: 2px solid #0f2236;            /* borde azul oscuro */
    border-radius: 14px;
    padding: 16px;
}

/* Títulos */
QLabel#titleLabel {
    color: #f3f0d0;                       /* crema */
    font-size: 28px;
    font-weight: 900;
    letter-spacing: 1px;
}
QLabel#subtitleLabel {
    color: #c8d0d8;
    font-size: 14px;
    margin-bottom: 8px;
}

/* Separador sutil bajo subtítulo (pseudo “línea”) */
QLabel#subtitleLabel {
    border-bottom: 1px solid rgba(255,255,255,0.08);
    padding-bottom: 8px;
}

/* ===== Tabla ===== */
QTableWidget#gamesTable {
    background: transparent;
    color: #e6edf3;
    gridline-color: rgba(255,255,255,0.05);
    selection-background-color: rgba(61, 139, 253, 120); /* azul */
    selection-color: white;
    alternate-background-color: rgba(255,255,255,0.03);
}
QTableWidget#gamesTable::item {
    padding: 6px;
}
QHeaderView::section {
    background: rgba(255,255,255,0.06);
    color: #e6e2c8;                        /* crema */
    border: none;
    border-bottom: 1px solid rgba(255,255,255,0.07);
    padding: 8px 10px;
    font-weight: 700;
}
QTableCornerButton::section {
    background: rgba(255,255,255,0.06);
    border: none;
}

/* ===== Botones ===== */
QPushButton {
    min-height: 44px;
    border-radius: 10px;
    font-weight: 800;
    font-size: 18px;
    padding: 6px 20px;
    color: white;
}
/* Primario: verde (Crear) */
QPushButton#createButton {
    background: #2ecc40;
    border: 3px solid #0d5f1a;
}
QPushButton#createButton:hover { background: #3fe24f; }
QPushButton#createButton:pressed { background: #1fa72f; }

/* Secundario: azul (Unirse) */
QPushButton#joinButton {
    background: #3a6ea7;
    border: 3px solid #1f3e61;
}
QPushButton#joinButton:hover { background: #497fbb; }
QPushButton#joinButton:pressed { background: #2f5c8b; }

/* Terciario: gris (Actualizar) */
QPushButton#refreshButton {
    background: #6b6f78;
    border: 3px solid #3a3d44;
}
QPushButton#refreshButton:hover { background: #7a7f88; }
QPushButton#refreshButton:pressed { background: #5a5e66; }

/* Back pill */
QPushButton#backButton {
    background: rgba(20,28,40,220);
    border: 2px solid #0f2236;
    border-radius: 14px;
    color: #e6edf3;
    padding: 6px 14px;
}
QPushButton#backButton:hover { background: rgba(30,40,55,240); }
</string>
